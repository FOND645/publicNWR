{
    "name": "nonwaranityrepair",
    "productName": "nonwaranityrepair",
    "version": "0.9-beta",
    "description": "Проект по автоматизации негарантийного ремонта",
    "main": "dist/app/main.js",
    "scripts": {
        "prettier": "npx prettier --write **/*.tsx **/*.ts",
        "start-app": "electron --trace-warnings dist/app/main.js",
        "start-server": "electron --trace-warnings dist/server/main.js",
        "pack)styles": "npx tailwindcss -i ./src/styles.css -o ./public/style.css",
        "build-dev": "npm run pack-dev && npm run install-build-deps && node copyBinaries.js && npm run pack-apps",
        "build-prod": "npm run pack-prod && npm run install-build-deps && npm run copy-binaries && npm run pack-apps",
        "install-build-deps": "cd dist/server && npm install && cd ../app && npm install && cd ../..",
        "pack-apps": "cd dist/server && npm run pack && cd ../app  && npm run pack && cd ../..",
        "copy-binaries": "node copyBinaries.js",
        "pack-dev": "webpack --config webpack.config.dev.ts",
        "pack-prod": "webpack --config webpack.config.prod.ts",
        "convert-db": "tsc helper/JsonToDB.ts & node helper/JsonToDB.js"
    },
